{
  "name": "tilnefna-web",
  "type": "static",
  "static": {
    "headers": [
      {
        "source" : "**/*.@(css|js)",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "max-age=86400"
          }
        ]
      },
      {
        "source" : "**/*.@(jpg|png)",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "max-age=86400"
          }
        ]
      }
    ]
  },
  "alias": "tilnefna.alheimsins.net"
}
