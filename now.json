{
  "name": "tilnefna-web",
  "type": "static",
  "static": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source" : "**/*.@(css|js)",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "max-age=86400"
          }
        ]
      }
    ]
  },
  "alias": "tilnefna.alheimsins.net",
  "files": [
    "dist"
  ]
}
