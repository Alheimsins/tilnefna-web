{
  "name": "tilnefna-web",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "deploy": "npm run build && cp now.json dist/now.json && now --static dist && now alias",
    "cleanup": "rm -rf dist",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "dependencies": {
    "axios": "0.18.0",
    "clone": "2.1.2",
    "gulp": "3.9.1",
    "register-service-worker": "1.5.2",
    "vue": "2.5.17",
    "vue-axios": "2.1.4",
    "vue-router": "3.0.1",
    "vuetify": "1.3.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.1.1",
    "@vue/cli-plugin-pwa": "3.1.1",
    "@vue/cli-service": "3.1.3",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "vue-cli-plugin-vuetify": "0.4.5",
    "vue-template-compiler": "2.5.17",
    "vuetify-loader": "1.0.6"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
