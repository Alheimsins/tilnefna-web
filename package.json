{
  "name": "tilnefna-web",
  "version": "0.2.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "deploy": "npm run build && cp now.json dist/now.json && now --static dist && now alias",
    "cleanup": "rm -rf dist",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "dependencies": {
    "axios": "1.1.0",
    "clone": "2.1.2",
    "gulp": "4.0.2",
    "register-service-worker": "1.7.2",
    "vue": "3.2.37",
    "vue-axios": "3.4.1",
    "vue-router": "4.4.5",
    "vuetify": "2.6.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-pwa": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "stylus": "0.59.0",
    "stylus-loader": "7.1.0",
    "vue-cli-plugin-vuetify": "2.5.5",
    "vue-template-compiler": "2.7.8",
    "vuetify-loader": "1.9.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
