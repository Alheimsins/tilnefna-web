{
  "name": "tilnefna-web",
  "version": "0.2.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "deploy": "npm run build && cp now.json dist/now.json && now --static dist && now alias",
    "cleanup": "rm -rf dist",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "dependencies": {
    "axios": "0.21.0",
    "clone": "2.1.2",
    "gulp": "4.0.2",
    "register-service-worker": "1.7.2",
    "vue": "2.6.12",
    "vue-axios": "3.2.0",
    "vue-router": "3.4.9",
    "vuetify": "2.3.22"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.9",
    "@vue/cli-plugin-pwa": "4.5.9",
    "@vue/cli-service": "4.5.9",
    "stylus": "0.54.8",
    "stylus-loader": "4.3.1",
    "vue-cli-plugin-vuetify": "2.0.8",
    "vue-template-compiler": "2.6.12",
    "vuetify-loader": "1.6.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
